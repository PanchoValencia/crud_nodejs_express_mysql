                div.carousel-wrap__container
                    div.carousel-wrap__container-pic
                        +pic("ironhide","jpg")
                div.carousel-wrap__container
                    div.carousel-wrap__container-pic
                        +pic("img2","jpg")
                div.carousel-wrap__container
                    div.carousel-wrap__container-pic
                        +pic("maxresdefault","jpg")
                div.carousel-wrap__container
                    div.carousel-wrap__container-pic
                        +pic("img3","jpg")
                div.carousel-wrap__container
                    div.carousel-wrap__container-pic
                        +pic("webprogramming","jpg")
                div.carousel-wrap__container
                    div.carousel-wrap__container-pic
                        +pic("img4","jpg")
  
            div.carousel-cont.owl-controls
                div.carousel-cont__container.owl-nav
                    p.prev.owl-prev anterior
                    p.next.owl-next siguiente
            div(href="#test-form-owl").popup-with-form.button-container
                p.button.wow.pulse Iniciar
  
