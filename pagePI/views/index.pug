                            input(name="new_password" type="password" placeholder="mi_password" required autocomplete="off" title="contraseña(máximo 10 caracteres)")#password.theme
                        li
                            button(type="submit").btn-form Guardar
  
  
    //-BOOTSTRAP
    section#bootstrap.bootstrap
        div.center
            div.title.wow.rubberBand
                div.title-container
                    h2 Bootstrap
                    p.desc Bootstrap es el framework de HTML, CSS Y JavaScript más popular para desarrollo responsivo en sitios web y móviles.
                    span.underline
  
            div.bootstrap__container
                div(data-wow-duration="2s").wrap.wow.slideInLeft
                    div.bootstrap__container-desc
                        h3.bootstrap__container-desc_h3.theme Estilos de botones
                    div.bootstrap__container-buttonClass
                        div.bootstrap__container-buttonClass-bc
