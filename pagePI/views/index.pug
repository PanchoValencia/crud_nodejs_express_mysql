extends layout

block content
    //- SECCION PORTADA
    section#portada.portada
        div.center
            div.portada-container
                div.portada-container-logo.wow.bounce
                    span.portada-container-logo__span FD
                div.portada-container-wrap
                    h1.portada-container-wrap__h1.wow.rubberBand Frameworks to Developers
                    p.portada-container-wrap__p.wow.zoomIn Aprende algunos de los frameworks y librerías más usadas de JavaScript y CSS.
  
  
    //-NAVBAR
    section.nav
        div.center
            div.nav-container
                div(title="configurar color de tema").nav-container__opts
                    i(title="Configurar color de tema")#rotate.fa.fa-cog.fa-spin.fa-3x.fa-fw
                    div(hidden).nav-container__opts-color
                        div.nav-container__opts-color__container
                            div.nav-container__opts-color__container-name
                                span(value="#4dc247").verde verde
                                span(value="#35A3C6").azulCielo azul cielo
                                span(value="#ff0084").rosa rosa
                                span(value="#0274B3").azul azul
                                span(value="#FFB000").amarillo amarillo
                                span(value="#cb2027").default default
                            div.nav-container__opts-color__container-color
                                span#verde
                                span#azulCielo
                                span#rosa
                                span#azul
                                span#amarillo
                                span#default
                div#nav.nav-container__items
                    p(href="#wow" ).nav-container__items-p Wow.js
                    p(href="#owl" ).nav-container__items-p Owl-carousel
                    p(href="#popup" ).nav-container__items-p Magnific-popup
                    p(href="#jquery" ).nav-container__items-p JQuery
                    p(href="#bootstrap" ).nav-container__items-p Bootstrap
                    p(href="#countTo" ).nav-container__items-p CountTo
                    p(href="#animate" ).nav-container__items-p Animate.css
                    p(href="#faq" ).nav-container__items-p FAQ
  
  
    //-WOW.JS
    section#wow.jswow
        div.center
            div.title.wow.rubberBand
                div.title-container
                    h2 Wow.js
                    p.desc Es un plugin de JavaScript que revela animaciones al desplazarse. Personaliza fácilmentre los ajustes de animación: estilo, retardo, longitud, desplazamiento, iteraciones...
                    span.underline
            div.jswow-images
                div.jswow-images__container
                    p#a(data-wow-duration="2s").jswow-images__container-p.wow.hinge hinge
                    p#b.jswow-images__container-p.wow.flip flip
                    p#c(data-wow-duration="2s").jswow-images__container-p.wow.slideInLeft slideInLeft
                div.jswow-images__container
                    p#d(data-wow-duration="1s" data-wow-iteration="2").jswow-images__container-p.wow.rubberBand rubberBand
                    p#e(data-wow-iteration="3" data-wow-duration="1s").jswow-images__container-p.wow.bounce bounce
                    p#f(data-wow-iteration="2").jswow-images__container-p.wow.swing swing
            div(href="#test-form-wow").popup-with-form.button-container
                p.button.wow.pulse Iniciar
  
            form(method="POST" action="http://localhost:3000/auth/signin")#test-form-wow.mfp-hide.form
                i(title="agregar usuario" href="#addUserWow").popup-with-form.fa.fa-user-plus.theme
                h3.form__h3.theme Wow.js
                fieldset.form__field
                    ol.form__field-ol
                        li
                            label(for="username").theme usuario:
                            input(name="username" type="text" placeholder="mi_usuario" required autocomplete="off")#name.theme
                        li
                            label(for="password").theme contraseña:
                            input(name="password" type="password" placeholder="mi_password" required autocomplete="off")#password.theme
                        li
                            button(type="submit").btn-form Entrar
  
            form(method="POST" action="http://localhost:3000/index")#addUserWow.mfp-hide.form
                i(title="regresar" href="#test-form-wow")#back.popup-with-form.fa.fa-arrow-left.theme
                h3.form__h3.theme Nuevo Usuario
                fieldset.form__field
                    ol.form__field-ol
                        li
                            label(for="user").theme usuario:
                            input(name="new_user" type="text" placeholder="mi_usuario" required autocomplete="off" title="nombre de usuario(máximo 20 caracteres)")#name.theme
                        li
                            label(for="password").theme contraseña:
                            input(name="new_password" type="password" placeholder="mi_password" required autocomplete="off" title="contraseña(máximo 10 caracteres)")#password.theme
                        li
                            button(type="submit").btn-form Guardar
                            
  
  
    //-BOTON-UP
    div.container
        div.container-icon
            i(href="#portada" title="click para ir a parte superior").fa.fa-arrow-circle-up.container-icon__up
  
  
    //-OWL.CAROUSEL
    section#owl.carousel
        div.center
            div.title.wow.rubberBand
                div.title-container
                    h2 Owl.carousel
                    p.desc Es un plugin de JQuery que nos permite tener un hermoso carrusel responsivo.
                    span.underline
  
            div.carousel-wrap.owl-carousel.owl-theme.owl-loaded
